---
import MyWorks from "../components/react/MyWorks.tsx";
import MyWorksBackend from "../components/react/MyWorksBackend.tsx";
import Layout from "../layouts/Layout.astro";
---

<Layout title="My Works" description="" thumbnail="">
    <div>
        <section class="py-20 px-4 bg-[#FCF8F1]">
            <h1 class="text-4xl font-bold text-center text-gray-900 mb-12">
                My Works
            </h1>
            <MyWorks />
        </section>
        <section class="py-20 px-4 bg-[#F6EDE0]">
            <h2 class="text-3xl font-bold text-center text-gray-800 mb-8">
                Backend & Systems
            </h2>
            <MyWorksBackend />
        </section>
    </div>
</Layout>
